{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://github-clone-wb09.onrender.com/api/:path*"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Credentials", "value": "true" },
          { "key": "Access-Control-Allow-Origin", "value": "github-clone-silk-six.vercel.app" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" }
        ]
      }
    ]
  }